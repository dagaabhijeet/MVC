@model List<MVC2.Models.Employee>
@*@model IEnumerable<MVC2.Models.Employee>*@
@{
    ViewBag.Title = "Index2";
}

<h2>List of Employee Objects</h2>

@foreach (var temp_e in Model)
{
<b>ID:</b>    @temp_e.id <br />
<b>Name:</b>    @temp_e.name <br />
<b>Marks:</b>    @temp_e.marks <br />
<b>Batch:</b>    @temp_e.batchcode <br />

    <hr>
}